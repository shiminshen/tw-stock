{
  "space": true,
  "prettier": true,
  "extends": [
    "xo/esnext",
    "xo-react"
  ],
  "rules": {
    "react/prop-types": 0,
    "unicorn/filename-case": 0,
    "import/no-unassigned-import": 0,
    "react/no-array-index-key": 0,
    "unicorn/explicit-length-check": 0,
    "unicorn/no-fn-reference-in-iterator" : 0
  }
}
